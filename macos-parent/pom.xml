<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.macos</groupId>
  <artifactId>macos-parent</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>macos-parent</name>
  <packaging>pom</packaging>
  <properties>
    <servlet-v>4.0.1</servlet-v>
    <jsp-v>2.3.1</jsp-v>
    <jstl-v>1.2</jstl-v>
    <json-lib>2.4</json-lib>
    <jackson-v>2.9.10</jackson-v>
    <mysql>8.0.16</mysql>
    <slf4j>1.7.5</slf4j>
    <druid>1.0.16</druid>
    <mybatis>3.5.2</mybatis>
    <macos>0.0.1-SNAPSHOT</macos>
    <pagehelper-version>5.1.10</pagehelper-version>
    <jsqlparser-version>3.0</jsqlparser-version>
    <jetty>9.4.19.v20190610</jetty>
    <nacos>1.1.3</nacos>
    <commons>3.9</commons>
    <lombok>1.18.4</lombok>
    <cglib>3.3.0</cglib>
    <beanutils>1.9.4</beanutils>
  </properties>

  <dependencyManagement>
    <dependencies>
      <!-- MacosX -->
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-framework</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-common</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-core</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter-web</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-aop</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-annotation</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-context</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-mvc</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-bean</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter-jdbc</artifactId>
        <version>${macos}</version>
      </dependency>
      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-println</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter-nacos-config</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter-npc</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter-nacos-cp</artifactId>
        <version>${macos}</version>
      </dependency>

      <dependency>
        <groupId>com.macos</groupId>
        <artifactId>macos-starter-router</artifactId>
        <version>${macos}</version>
      </dependency>

      <!--Nacos-->
      <dependency>
        <groupId>com.alibaba.nacos</groupId>
        <artifactId>nacos-client</artifactId>
        <version>${nacos}</version>
      </dependency>
      <!-- servlet -->
      <dependency>
        <groupId>javax.servlet</groupId>
        <artifactId>javax.servlet-api</artifactId>
        <version>${servlet-v}</version>
      </dependency>

      <dependency>
        <groupId>javax.annotation</groupId>
        <artifactId>javax.annotation-api</artifactId>
        <version>1.3.2</version>
        <scope>compile</scope>
      </dependency>

      <dependency>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-webapp</artifactId>
        <version>${jetty}</version>
      </dependency>

      <dependency>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-servlet</artifactId>
        <version>${jetty}</version>
      </dependency>
      <!-- https://mvnrepository.com/artifact/org.eclipse.jetty/jetty-http -->
      <dependency>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-http</artifactId>
        <version>${jetty}</version>
      </dependency>

      <dependency>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-io</artifactId>
        <version>${jetty}</version>
      </dependency>

      <dependency>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-security</artifactId>
        <version>${jetty}</version>
      </dependency>

      <dependency>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-util</artifactId>
        <version>${jetty}</version>
      </dependency>

      <!-- https://mvnrepository.com/artifact/com.fasterxml.jackson.core/jackson-core -->
      <dependency>
        <groupId>com.fasterxml.jackson.core</groupId>
        <artifactId>jackson-core</artifactId>
        <version>${jackson-v}</version>
      </dependency>

      <!-- https://mvnrepository.com/artifact/com.fasterxml.jackson.core/jackson-annotations -->
      <dependency>
        <groupId>com.fasterxml.jackson.core</groupId>
        <artifactId>jackson-annotations</artifactId>
        <version>${jackson-v}</version>
      </dependency>

      <dependency>
        <groupId>com.fasterxml.jackson.core</groupId>
        <artifactId>jackson-databind</artifactId>
        <version>${jackson-v}</version>
      </dependency>

      <dependency>
        <groupId>mysql</groupId>
        <artifactId>mysql-connector-java</artifactId>
        <version>${mysql}</version>
      </dependency>

      <dependency>
        <groupId>com.alibaba</groupId>
        <artifactId>druid</artifactId>
        <version>${druid}</version>
      </dependency>

      <!-- https://mvnrepository.com/artifact/org.mybatis/mybatis -->
      <dependency>
        <groupId>org.mybatis</groupId>
        <artifactId>mybatis</artifactId>
        <version>${mybatis}</version>
      </dependency>

      <!--pagehelper-->
      <dependency>
        <groupId>com.github.pagehelper</groupId>
        <artifactId>pagehelper</artifactId>
        <version>${pagehelper-version}</version>
      </dependency>

      <!-- pagehelper的依赖包:jsqlparser -->
      <dependency>
        <groupId>com.github.jsqlparser</groupId>
        <artifactId>jsqlparser</artifactId>
        <version>${jsqlparser-version}</version>
      </dependency>

      <dependency>
        <groupId>org.projectlombok</groupId>
        <artifactId>lombok</artifactId>
        <version>${lombok}</version>
      </dependency>


      <dependency>
        <groupId>org.slf4j</groupId>
        <artifactId>slf4j-api</artifactId>
        <version>${slf4j}</version>
      </dependency>

      <dependency>
        <groupId>org.slf4j</groupId>
        <artifactId>slf4j-simple</artifactId>
        <version>${slf4j}</version>
      </dependency>
      <dependency>
        <groupId>org.apache.commons</groupId>
        <artifactId>commons-lang3</artifactId>
        <version>${commons}</version>
      </dependency>
      <!-- https://mvnrepository.com/artifact/commons-beanutils/commons-beanutils -->
      <dependency>
        <groupId>commons-beanutils</groupId>
        <artifactId>commons-beanutils</artifactId>
        <version>${beanutils}</version>
      </dependency>

      <!-- https://mvnrepository.com/artifact/cglib/cglib -->
      <dependency>
        <groupId>cglib</groupId>
        <artifactId>cglib</artifactId>
        <version>${cglib}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <distributionManagement>
    <repository>
      <id>MacosX</id>
      <name>Release Deploy</name>
      <url>http://duanya.online:9700/repository/maven-releases/</url>
    </repository>
    <snapshotRepository>
      <id>MacosX</id>
      <name>Snapshot Deploy</name>
      <url>http://duanya.online:9700/repository/maven-snapshots/</url>
    </snapshotRepository>
  </distributionManagement>


  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-compiler-plugin</artifactId>
          <configuration>
            <source>1.8</source>
            <target>1.8</target>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>

  </build>
</project>
